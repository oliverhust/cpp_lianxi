objectclass ( 1.5.1
	NAME 'Portal'
	DESC 'An interface through which access to Storage Nodes within the Network Entity can be obtained.'
	STRUCTURAL
	MUST ( IPAddress $ Port $ PortalSymbolicName $ ESIInterval $ ESIPort $ PortalIndex $ SCNPort $ PortalSecurityBitmap ) 
	MAY ( EntityID ))
	
